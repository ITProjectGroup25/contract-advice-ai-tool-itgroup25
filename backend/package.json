{
  "name": "@contract/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typecheck": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "db:generate": "dotenv -e ../.env -- drizzle-kit generate",
    "db:migrate": "dotenv -e ../.env -- drizzle-kit migrate",
    "db:push": "dotenv -e ../.env -- drizzle-kit push",
    "db:studio": "dotenv -e ../.env -- drizzle-kit studio",
    "update-types": "npx drizzle-kit pull && npx shx mv ./drizzle/*.ts ./src/db/"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "0.7.0",
    "@tanstack/react-query": "^5.90.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.6",
    "next-auth": "^4.24.11",
    "pg": "^8.11.3",
    "postgres": "^3.4.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.31.5",
    "shx": "^0.4.0",
    "supabase": "^2.39.2",
    "typescript": "^5"
  },
  "overrides": {
    "next": "14.0.4"
  }
}
